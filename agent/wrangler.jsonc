{
	"$schema": "node_modules/wrangler/config-schema.json",
	"ai": {
    	"binding": "AI"
    },
	"name": "agent",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-27",
	"compatibility_flags": ["nodejs_compat", "durable_object_alarms"],
	"migrations": [
		{
			"new_sqlite_classes": [
				"QuestMasterAgent"
			],
			"tag": "v1"
		},
		{
			"renamed_classes": [
				{
					"from": "TaskMasterAgent",
					"to": "QuestMasterAgent"
				}
			],
			"tag": "v2"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "QuestMasterAgent",
				"name": "QuestMasterAgent"
			}
		]
	},
	"ratelimits": [
		{
			"name": "CHAT_RATE_LIMITER",
			"namespace_id": "1001",
			"simple": {
				"limit": 30,
				"period": 60
			}
		},
		{
			"name": "TASK_RATE_LIMITER",
			"namespace_id": "1002",
			"simple": {
				"limit": 60,
				"period": 60
			}
		}
	],
	"observability": {
		"enabled": true
	}
}